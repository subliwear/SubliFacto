# Application Configuration
APP_NAME=SubFactory
APP_ENV=local
APP_KEY=base64:your_app_key_example
APP_DEBUG=true
APP_ID=your_app_id_example
APP_URL=http://localhost:8000

# Logging Configuration
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USERNAME=your_db_username_example
DB_DATABASE=your_database_name_example
DB_PASSWORD=your_db_password_example

# Cache and Session Configuration
BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Memcached
MEMCACHED_HOST=127.0.0.1

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=your_mail_host_example
MAIL_PORT=465
MAIL_USERNAME=your_mail_username_example
MAIL_PASSWORD=your_mail_password_example
MAIL_ENCRYPTION=ssl
MAIL_FROM_ADDRESS=your_email@example.com
MAIL_FROM_NAME=no-reply

# External Services Configuration
MAILGUN_DOMAIN=your_mailgun_domain_example
MAILGUN_SECRET=your_mailgun_secret_example
MAILGUN_ENDPOINT=api.mailgun.net

PUSHER_APP_ID=your_pusher_app_id_example
PUSHER_APP_KEY=your_pusher_app_key_example
PUSHER_APP_SECRET=your_pusher_app_secret_example
PUSHER_HOST=your_pusher_host_example
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id_example
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_example
AWS_BUCKET=your_bucket_name_example
AWS_DEFAULT_REGION=your_aws_region_example
AWS_USE_PATH_STYLE_ENDPOINT=true

# ReCaptcha
GOOGLE_RECAPTCHA_SECRET=your_recaptcha_secret_example
GOOGLE_RECAPTCHA_KEY=your_recaptcha_key_example

# Payment Gateways
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=your_paypal_client_id_example
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_example
PAYPAL_WEBHOOK_ID=your_paypal_webhook_id_example

STRIPE_API_KEY=your_stripe_api_key_example
STRIPE_SECRET_KEY=your_stripe_secret_key_example
STRIPE_WEBHOOK_SECRET_KEY=your_stripe_webhook_secret_key_example

RAZORPAY_KEY=your_razorpay_key_id_example
RAZORPAY_SECRET=your_razorpay_key_secret_example
RAZORPAY_WEBHOOK_SECRET_KEY=your_razorpay_webhook_secret_key_example

MOLLIE_KEY=your_mollie_api_key_example
MOLLIE_WEBHOOK_URL=your_mollie_webhook_url_example

CCAVENUE_SANDBOX_MODE=1
CCAVENUE_MERCHANT_ID=your_merchant_id_example
CCAVENUE_ACCESS_CODE=your_access_code_example
CCAVENUE_WORKING_KEY=your_working_key_example

BKASH_SANDBOX_MODE=1
BKASH_APP_KEY=your_bkash_app_key_example
BKASH_APP_SECRET=your_bkash_app_secret_example
BKASH_USERNAME=your_bkash_username_example
BKASH_PASSWORD=your_bkash_password_example

FLW_SANDBOX_MODE=1
FLW_PUBLIC_KEY=your_flutterwave_public_key_example
FLW_SECRET_KEY=your_flutterwave_secret_key_example
FLW_SECRET_HASH=your_flutterwave_secret_hash_example

PAYSTACK_SANDBOX_MODE=1
PAYSTACK_PUBLIC_KEY=your_paystack_public_key_example
PAYSTACK_SECRET_KEY=your_paystack_secret_key_example
PAYSTACK_PAYMENT_URL=your_paystack_payment_url_example

SSLC_STORE_ID=your_sslcommerce_store_id_example
SSLC_STORE_PASSWORD=your_sslcommerce_store_password_example
SSLC_SANDBOX_MODE=1

TWILIO_SID=your_twilio_sid_example
TWILIO_AUTH_TOKEN=your_twilio_auth_token_example
TWILIO_NUMBER=your_twilio_number_example

# Dummy Data
DUMMY_IMAGES_URL=https://example.com/path/to/dummy/images.zip
DOWNLOAD_EXPIRE_TIME=2
MEDIA_DISK=public
